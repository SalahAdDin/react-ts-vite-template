{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.2.1",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "git@github.com:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:preview": "vitest-preview",
    "test:e2e": "playwright test --trace on --pass-with-no-tests",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "pnpm prettier --write",
    "format": "pnpm prettier:fix &&  pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "release": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.0",
    "@tanstack/react-query": "4.29.12",
    "@tanstack/router": "0.0.1-beta.89",
    "axios": "1.4.0",
    "dotenv": "16.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.44.2",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@commitlint/types": "17.4.4",
    "@faker-js/faker": "8.0.2",
    "@hookform/devtools": "4.3.1",
    "@playwright/test": "1.34.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.1.0",
    "@semantic-release/npm": "10.0.3",
    "@semantic-release/release-notes-generator": "11.0.2",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/eslint-plugin-query": "4.29.9",
    "@tanstack/react-query-devtools": "4.29.12",
    "@tanstack/router-devtools": "0.0.1-beta.89",
    "@testing-library/dom": "9.3.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.31.4",
    "@vitest/ui": "0.31.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-vitest": "0.2.5",
    "happy-dom": "9.20.3",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "msw": "1.2.1",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "semantic-release": "21.0.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.31.4",
    "vitest-dom": "0.1.0",
    "vitest-preview": "0.0.1",
    "web-vitals": "3.3.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"]
}
