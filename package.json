{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.2.3",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "git@github.com:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:preview": "vitest-preview",
    "test:e2e": "playwright test --trace on --pass-with-no-tests",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier src --check",
    "format:fix": "pnpm prettier --write",
    "format:lint": "pnpm prettier:fix && pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "release": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@tanstack/react-query": "5.51.11",
    "@tanstack/react-router": "1.45.7",
    "axios": "1.7.2",
    "dotenv": "16.4.5",
    "million": "3.1.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.1",
    "valibot": "0.36.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/types": "19.0.3",
    "@faker-js/faker": "8.4.1",
    "@hookform/devtools": "4.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@playwright/test": "1.45.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.1.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@tailwindcss/forms": "0.5.7",
    "@tanstack/eslint-plugin-query": "5.51.12",
    "@tanstack/react-query-devtools": "5.51.11",
    "@tanstack/router-devtools": "1.45.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.0.4",
    "@vitest/ui": "2.0.4",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-playwright": "1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-vitest": "0.5.4",
    "happy-dom": "14.12.3",
    "husky": "9.1.1",
    "lint-staged": "15.2.7",
    "msw": "2.3.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "semantic-release": "24.0.0",
    "tailwindcss": "3.4.6",
    "typescript": "5.5.3",
    "vite": "5.3.4",
    "vitest": "2.0.4",
    "vitest-dom": "0.1.1",
    "vitest-preview": "0.0.1",
    "web-vitals": "4.2.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"],
  "packageManager": "pnpm@9.6.0"
}
