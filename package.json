{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.2.3",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "git@github.com:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:preview": "vitest-preview",
    "test:e2e": "playwright test --trace on --pass-with-no-tests",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier src --check",
    "format:fix": "pnpm prettier --write",
    "format:lint": "pnpm prettier:fix && pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "release": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.2",
    "@tanstack/react-query": "5.8.7",
    "@tanstack/react-router": "0.0.1-beta.214",
    "axios": "1.6.2",
    "dotenv": "16.3.1",
    "million": "2.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "valibot": "0.21.0",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@commitlint/types": "18.4.3",
    "@faker-js/faker": "8.3.1",
    "@hookform/devtools": "4.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@playwright/test": "1.40.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.3",
    "@semantic-release/npm": "11.0.1",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@tailwindcss/forms": "0.5.7",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@tanstack/react-query-devtools": "5.8.7",
    "@tanstack/router-devtools": "0.0.1-beta.214",
    "@testing-library/dom": "9.3.3",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/node": "20.10.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@vitejs/plugin-react": "4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "eslint": "8.54.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "0.18.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-vitest": "0.3.10",
    "happy-dom": "12.10.3",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "msw": "2.0.9",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "semantic-release": "22.0.8",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "vite": "5.0.2",
    "vitest": "0.34.6",
    "vitest-dom": "0.1.1",
    "vitest-preview": "0.0.1",
    "web-vitals": "3.5.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"]
}
