{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.2.1",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "git@github.com:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:preview": "vitest-preview",
    "test:e2e": "playwright test --trace on --pass-with-no-tests",
    "test:e2e:watch": "playwright test --ui",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "pnpm prettier --write",
    "format": "pnpm prettier:fix &&  pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "release": "semantic-release --dry-run --no-ci"
  },
  "dependencies": {
    "@hookform/resolvers": "3.0.0",
    "@tanstack/react-query": "4.28.0",
    "@tanstack/react-router": "0.0.1-beta.83",
    "axios": "1.3.4",
    "dotenv": "16.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.7",
    "yup": "1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/types": "17.4.4",
    "@faker-js/faker": "7.6.0",
    "@hookform/devtools": "4.3.1",
    "@playwright/test": "1.32.0",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "10.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/eslint-plugin-query": "4.27.0",
    "@tanstack/react-query-devtools": "4.28.0",
    "@tanstack/react-router-devtools": "0.0.1-beta.83",
    "@testing-library/dom": "9.0.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.29.7",
    "@vitest/ui": "0.29.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.2",
    "eslint-plugin-vitest": "0.0.54",
    "happy-dom": "8.9.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "msw": "1.2.0",
    "prettier": "2.8.6",
    "prettier-plugin-tailwindcss": "0.2.5",
    "semantic-release": "20.1.3",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vitest": "0.29.7",
    "vitest-dom": "0.0.4",
    "vitest-preview": "0.0.1",
    "web-vitals": "3.3.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"]
}
