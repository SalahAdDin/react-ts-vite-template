{
  "name": "react-ts-vite-template",
  "description": "Boilerplate for React/Typescript, built on top of Vite ⚡️",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Luis Alaguna <alagunasalahaddin@live.com>",
  "homepage": "https://github.com/SalahAdDin/react-ts-vite-template#readme",
  "repository": "github:SalahAdDin/react-ts-vite-template.git",
  "bugs": "https://github.com/SalahAdDin/react-ts-vite-template/issues",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage --ui",
    "test:ui": "vitest --watch --ui",
    "test:e2e": "playwright test --trace on",
    "test:e2e:install": "playwright install",
    "test:e2e:help": "playwright --help",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "pnpm prettier --write",
    "format": "pnpm prettier:fix &&  pnpm lint:fix",
    "lint-staged": "lint-staged",
    "type-check": "tsc",
    "prepare": "husky install",
    "commit": "commitlint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@tanstack/react-query": "4.22.0",
    "axios": "1.2.3",
    "dotenv": "16.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@commitlint/types": "17.4.0",
    "@faker-js/faker": "7.6.0",
    "@hookform/devtools": "4.3.0",
    "@playwright/test": "1.29.2",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/eslint-plugin-query": "4.21.0",
    "@tanstack/react-query-devtools": "4.22.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "@vitejs/plugin-react": "3.0.1",
    "@vitest/coverage-c8": "0.27.2",
    "@vitest/ui": "0.27.2",
    "autoprefixer": "10.4.13",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.12.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-vitest": "0.0.29",
    "happy-dom": "8.1.4",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "msw": "0.49.3",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.27.2",
    "vitest-dom": "0.0.4",
    "web-vitals": "3.1.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "keywords": ["react", "boilerplate", "vite", "bundle", "typescript"]
}
